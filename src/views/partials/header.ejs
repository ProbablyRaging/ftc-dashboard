    <script>
        const body = document.body;
        const currentTheme = localStorage.getItem('currentTheme');
        const customColor = localStorage.getItem('customColor');
        const currentIndex = localStorage.getItem('currentIndex');
        if (!currentIndex) localStorage.setItem('currentIndex', 0);
        if (currentTheme) {
            body.classList.add(currentTheme);
            // Add custom color
            if (!customColor) localStorage.setItem('customColor', 'blue');
            if (currentTheme === 'dark') {
                body.classList.add(customColor);
            } else {
                body.classList.add(`${customColor}-nav`);
            }
        } else {
            localStorage.setItem('currentTheme', 'light');
            body.classList.add('light');
        }
    </script>

